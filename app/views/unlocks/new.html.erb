<body class="body--unlock"></body>

<div class="form">
  <!-- form -->
  <div class="nes-container is-rounded">
    <h1 class="title"><%= t('unlock_user') %></h1>
    <%= form_with(scope: resource_name, url: user_unlock_path, method: :post) do |f| %>
      <%= render "shared/error_messages", object: resource %>

      <%= render 'form', f: f %>
    <% end %>
  </div>
  <%= render "shared/devise_links" %>
</div>
